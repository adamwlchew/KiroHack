# AWS Configuration
AWS_REGION=ap-southeast-2

# Model Configuration
CLAUDE_MODEL_ID=anthropic.claude-3-sonnet-20240229-v1:0
CLAUDE_FALLBACK_MODEL_ID=anthropic.claude-3-haiku-20240307-v1:0
CLAUDE_MAX_TOKENS=4000
CLAUDE_TEMPERATURE=0.7
CLAUDE_TOP_P=1
CLAUDE_TOP_K=250

TITAN_MODEL_ID=amazon.titan-text-express-v1
TITAN_FALLBACK_MODEL_ID=amazon.titan-text-lite-v1
TITAN_MAX_TOKENS=3000
TITAN_TEMPERATURE=0.7
TITAN_TOP_P=1

STABLE_DIFFUSION_MODEL_ID=stability.stable-diffusion-xl-v1
STABLE_DIFFUSION_WIDTH=1024
STABLE_DIFFUSION_HEIGHT=1024
STABLE_DIFFUSION_CFG_SCALE=7
STABLE_DIFFUSION_STEPS=30

COHERE_MODEL_ID=cohere.command-text-v14
COHERE_FALLBACK_MODEL_ID=cohere.command-light-text-v14
COHERE_MAX_TOKENS=2000
COHERE_TEMPERATURE=0.7
COHERE_P=0.75
COHERE_K=0

# Cost Management
BEDROCK_DAILY_LIMIT=100
BEDROCK_MONTHLY_LIMIT=2000
BEDROCK_WARNING_THRESHOLD=80

# Caching Configuration
BEDROCK_CACHE_ENABLED=true
BEDROCK_CACHE_TTL=3600
BEDROCK_CACHE_MAX_SIZE=1000

# Retry Policy
BEDROCK_MAX_RETRIES=3
BEDROCK_BASE_DELAY=1000
BEDROCK_MAX_DELAY=10000

# Service Configuration
PORT=3000
NODE_ENV=development
LOG_LEVEL=info